{
  git {
    repo wenger.dev {
      base_dir /tmp
      url https://github.com/GreedTP/wenger.dev
      branch main
      post pull exec {
        name Pager
        command /usr/bin/echo
        args "pulled wenger.dev repo"
      }
    }
  }
}

wenger.dev {
  route /version* {
    respond * "1.0.0" 200
  }
  route /update/wenger.dev {
    git update repo wenger.dev
  }
  route {
    file_server {
      root /tmp/wenger.dev
    }
  }
}